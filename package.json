{
  "name": "foxone-uikit-next",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:legacy": "export NODE_OPTIONS=--openssl-legacy-provider && turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run build lint && changeset version && changeset publish",
    "storybook": "start-storybook -p 6006",
    "storybook:legacy": "export NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "docs": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "babel-loader": "^8.3.0",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^9.0.1",
    "eslint-utils": "^3.0.0",
    "prettier": "^2.8.4",
    "turbo": "latest",
    "typescript": "^4.8.4",
    "vite": "^3.2.5",
    "vite-plugin-vuetify": "^1.0.0",
    "vue": "^3.2.40",
    "vue-loader": "^16.8.3",
    "vuetify": "^3.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.25.0",
    "@foxone/dev": "^0.2.22"
  },
  "packageManager": "yarn@1.22.1"
}
